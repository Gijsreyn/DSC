{
    "manifestVersion": "1.0",
    "type": "DSC/PowerShellGroup",
    "version": "0.1.0",
    "description": "Resource provider to classic DSC Powershell resources.",
    "provider": {
      "list": {
        "executable": "pwsh",
        "args": [
          "-NoLogo",
          "-NonInteractive",
          "-NoProfile",
          "-Command",
          "./powershellgroup.resource.ps1 List"
        ]
        },
      "config": "full"
    },
    "get": {
      "executable": "pwsh",
      "args": [
        "-NoLogo",
        "-NonInteractive",
        "-NoProfile",
        "-Command",
        "$Input | ./powershellgroup.resource.ps1 Get"
      ]
      },
    "exitCodes": {
      "0": "Success"
    }
  }
  